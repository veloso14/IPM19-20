<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/themecolor" />
            <padding android:left="2dp" android:right="2dp" android:top="2dp" android:bottom="2dp"/>
            <stroke android:width="2dp" android:color="@color/themecolor"/>

        </shape>
    </item>
    <item android:left="5dp" android:right="5dp"  android:bottom="5dp" android:top="5dp" >
        <shape android:shape="rectangle">
            <corners android:radius="15dp"/>
            <solid android:color="#ffffff" />
        </shape>
    </item>
</layer-list>
